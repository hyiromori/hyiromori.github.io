{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "locale": "ja",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "target": "es6",
        "typeRoots": [
            "app/@types"
        ]
    }
}
